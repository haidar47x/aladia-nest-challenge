{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/aladia-nest-challenege/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/aladia-nest-challenege/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/aladia-nest-challenge",
  "projects": {
    "aladia-nest-challenege": {
      "type": "application",
      "root": "apps/aladia-nest-challenge",
      "entryFile": "main",
      "sourceRoot": "apps/aladia-nest-challenge/src",
      "compilerOptions": {
        "tsConfigPath": "apps/aladia-nest-challenge/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    }
  }
}